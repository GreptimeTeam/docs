{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "paths": {
      "@/*": ["./.vitepress/theme/*"],
    }
  },
  "vueCompilerOptions": {
    "experimentalDisableTemplateSupport": true
  },
  "include": ["./*.ts", "./.vitepress/**/*.vue","./.vitepress/**/*.ts", "./.vitepress/**/*.ts", "en/config/*.ts", "zh/config/*.ts"]
}
